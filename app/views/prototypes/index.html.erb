<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.name}さん", "/users/#{current_user.id}", class: :greeting__link%>
      </div>
    <% else %>
    <% end %>
    <div class="card__wrapper">
      <% @prototype.each do |i| %>
        <%= render partial: "prototype", locals: { proto: i } %>
      <% end %>
    </div>
  </div>
</main>
